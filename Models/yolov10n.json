{
  "inputs": { "images": [1, 3, 640, 640] },
  "default_loader": {
    "dataset_path": "../sample/calibration_dataset",
    "file_extensions": ["jpeg", "jpg", "png", "JPEG", "JPG", "PNG"],
    "preprocessings": [
      { "resize": { "width": 640, "height": 640 } },
      { "convertColor": { "form": "BGR2RGB" } },
      { "div": { "x": 255.0 } },
      { "transpose": { "axis": [2, 0, 1] } },
      { "expandDim": { "axis": 0 } }
    ]
  },
  "calibration_num": 189,
  "calibration_method": "ema"
}
